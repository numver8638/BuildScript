#include <BuildScript/Version.h>

using namespace BuildScript;

// static
int Version::GetVersionMajor() { return @BuildScript_VERSION_MAJOR@; }

// static 
int Version::GetVersionMinor() { return @BuildScript_VERSION_MINOR@; }

// static
int Version::GetVersionPatch() { return @BuildScript_VERSION_PATCH@; }

// static
const char* const Version::GetVersion() { return u8"@BuildScript_VERSION@"; }

// static
const char* const Version::GetOSType() { return u8"@OSTYPE@"; }

// static
const char* const Version::GetArchType() { return u8"@ARCHTYPE@"; }